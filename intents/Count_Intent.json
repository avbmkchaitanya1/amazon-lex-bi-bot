{
	"name": "Count_Intent",
        "description": "Recursively query the dataset",
        "conclusionStatement": {
          "messages": [
            {
              "content": "via code hook",
              "contentType": "PlainText"
            }
          ]
        },
        "fulfillmentActivity": {
          "codeHook": {
            "messageVersion": "1.0",
            "uri": "{{lambda-arn}}"
          },
          "type": "CodeHook"
        },
        "sampleUtterances": [
          "How many {account_holder_id} have the {paystation_service_level} disbursement",
          "How many {detailed_result_code} for {account_holder_id}",
          "How many {detailed_result_code} for {account_holder_id} in {disbursement_execution_date}",
          "Total disbursements for {account_holder_id} in {disbursement_execution_date}",
          "Total disbursements in {disbursement_execution_date}",
          "How many disbursements"
        ],
    "slots": [
        {
            "slotType": "account_holder_id",
            "name": "account_holder_id",
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The name of the seller",
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 1, 
            "slotTypeVersion": "$LATEST", 
            "sampleUtterances": [], 
            "description": "account_holder_id"
        },
        {
            "slotType": "paystation_service_level",
            "name": "paystation_service_level",
            "slotConstraint": "Optional", 
            "valueElicitationPrompt": {
                "maxAttempts": 2, 
                "messages": [
                    {
                        "content": "The payment rail of the disbursement",
                        "contentType": "PlainText"
                    }
                ]
            }, 
            "priority": 3, 
            "sampleUtterances": [], 
            "description": "paystation_service_level"
        }
    ]
}
